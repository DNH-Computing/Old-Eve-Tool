<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="LONGVARCHAR" hibernate-type="string"
			length="65535" not-null="true">
		</sql-type>
		<sql-type jdbc-type="BIT" hibernate-type="boolean"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			length="64" not-null="true">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="string" length="64"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="string" length="11"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="string" length="128"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			length="50" not-null="true">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="string" length="7"
			not-null="true">
		</sql-type>
	</type-mapping>
	<table-filter match-catalog="EVETool"
		match-name="AuthorisedCorporations" />
	<table-filter match-catalog="EVETool" match-name="Corporation" />
	<table-filter match-catalog="EVETool" match-name="CorporationList" />
	<table-filter match-catalog="EVETool" match-name="Module" />
	<table-filter match-catalog="EVETool" match-name="ModuleParent" />
	<table-filter match-catalog="EVETool" match-name="Permission" />
	<table-filter match-catalog="EVETool" match-name="Role" />
	<table-filter match-catalog="EVETool" match-name="RoleParent" />
	<table-filter match-catalog="EVETool" match-name="Rule" />
	<table-filter match-catalog="EVETool" match-name="User" />
	<table-filter match-catalog="EVETool" match-name="UserRole" />
	<table-filter match-schema="EVETool" match-name=".*"></table-filter>
	<table catalog="EVETool" name="RoleParent"></table>
	<table catalog="EVETool" name="Permission"></table>
	<table catalog="EVETool" name="Module"></table>
	<table catalog="EVETool" name="CorporationList"></table>
	<table catalog="EVETool" name="User"></table>
	<table catalog="EVETool" name="ModuleParent"></table>
	<table catalog="EVETool" name="UserRole"></table>
	<table catalog="EVETool" name="Rule"></table>
	<table catalog="EVETool" name="Corporation"></table>
	<table catalog="EVETool" name="AuthorisedCorporations"></table>
	<table catalog="EVETool" name="Role"></table>
</hibernate-reverse-engineering>