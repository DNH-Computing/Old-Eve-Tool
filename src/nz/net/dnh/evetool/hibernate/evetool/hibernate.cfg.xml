<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">org.gjt.mm.mysql.Driver</property>
        <property name="hibernate.connection.password">_dnh</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost/EVETool</property>
        <property name="hibernate.connection.username">dnh</property>
        <property name="hibernate.default_schema">EVETool</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQL5Dialect</property>
        <property name="hibernate.search.autoregister_listeners">false</property>
        <mapping class="nz.net.dnh.evetool.hibernate.AuthorisedCorporations" />
        <mapping class="nz.net.dnh.evetool.hibernate.User" />
        <mapping class="nz.net.dnh.evetool.hibernate.ModuleParent" />
        <mapping class="nz.net.dnh.evetool.hibernate.RoleParent" />
        <mapping class="nz.net.dnh.evetool.hibernate.Permission" />
        <mapping class="nz.net.dnh.evetool.hibernate.UserRole" />
        <mapping class="nz.net.dnh.evetool.hibernate.Role" />
        <mapping class="nz.net.dnh.evetool.hibernate.Module" />
        <mapping class="nz.net.dnh.evetool.hibernate.Corporation" />
        <mapping class="nz.net.dnh.evetool.hibernate.Rule" />
        <mapping class="nz.net.dnh.evetool.hibernate.CorporationList" />
    </session-factory>
</hibernate-configuration>
